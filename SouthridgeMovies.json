{
	"name": "SouthridgeMovies",
	"properties": {
		"activities": [
			{
				"name": "copymovies",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"preferredRegions": []
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "movies",
						"type": "DatasetReference",
						"parameters": {
							"collection": {
								"value": "@pipeline().parameters.table_name",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "IngestionTarget",
						"type": "DatasetReference",
						"parameters": {
							"targetfolder": {
								"value": "@concat(concat(concat(toUpper(substring(pipeline().parameters.SourceDatabase,0,1)),substring(pipeline().parameters.SourceDatabase,1,9)), concat('/', pipeline().parameters.table_name)))",
								"type": "Expression"
							},
							"filename": {
								"value": "@concat(pipeline().parameters.table_name,'.csv')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"SourceDatabase": {
				"type": "string"
			},
			"table_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"lastPublishTime": "2020-09-21T15:42:07Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}